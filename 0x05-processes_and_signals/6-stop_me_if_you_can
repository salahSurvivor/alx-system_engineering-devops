#!/usr/bin/env bash
# This Bash script stops the "4-to_infinity_and_beyond" process without using kill.

# Find the PID of the "4-to_infinity_and_beyond" process
PID=$(pgrep -f "4-to-infinity-and-beyond")

if [ -z "$PID" ]; then
    echo "Process not found."
else
    # Send a termination signal to the process
    kill -SIGTERM $PID
    echo "4-to-infinity_and_beyond process has been terminated."
fi
